{
    "name": "symfony-cmf/block-bundle",
    "type": "symfony-bundle",
    "description": "Symfony CMF Block Bundle",
    "keywords": [
        "Symfony CMF",
        "Block",
        "content fragments"
    ],
    "homepage": "http://cmf.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/BlockBundle/contributors"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/BlNARYFOUR/menu-bundle.git"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "symfony/framework-bundle": "^2.8 || ^3.3 || ^4.0",
        "doctrine/phpcr-bundle": "^1.3 || ^2.0",
        "doctrine/phpcr-odm": "^1.4.2 || ^2.0",
        "sonata-project/block-bundle": "^3.11",
        "symfony-cmf/core-bundle": "^3.0",
        "jackalope/jackalope-fs": "^0.0.3",
        "jackalope/jackalope": "^1.4.1"
    },
    "require-dev": {
        "phpcompatibility/php-compatibility": "^9.3",
        "symfony-cmf/menu-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.3 || ^4.0",
        "sonata-project/cache-bundle": "^3.3",
        "twig/twig": "^1.14.2 || ^2.0",
        "symfony/security-bundle": "^2.8 || ^3.3 || ^4.0",
        "symfony/twig-bundle": "^2.8 || ^3.3 || ^4.0",
        "symfony/asset": "^2.8 || ^3.3 || ^4.0",
        "symfony/templating": "^2.8 || ^3.3 || ^4.0"
    },
    "suggest": {
        "sonata-project/cache-bundle": "To add caching support for block loading",
        "eko/feedbundle": "When using the RssBlock"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\BlockBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\BlockBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "conflict": {
        "sonata/cache": "<1.1.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
